{
	"$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
	"runtime.version": "LuaJIT",
	"runtime.path": [
		"lua/?.lua",
		"lua/?/init.lua"
	],
	"diagnostics.globals": [
		"vim"
	],
	"format.defaultConfig": {
		"indent_style": "tab",
		"quote_style": "single",
		"end_of_line": "lf",
		"trailing_table_separator": "smart"
	},
	"workspace.checkThirdParty": false,
	"workspace.library": [
		"$VIMRUNTIME"
	]
}
